    <groupId>org.citadel.features.distributed</groupId>
    <artifactId>org.citadel.features.distributed.dao-test</artifactId>
    <packaging>bundle</packaging>
    <name>OpenNMS :: Features :: Distributed :: DAO :: Test</name>
    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.felix</groupId>
                <artifactId>maven-bundle-plugin</artifactId>
                <extensions>true</extensions>
                <configuration>
                    <instructions>
                        <DynamicImport-Package>*</DynamicImport-Package>
                    </instructions>
                </configuration>
            </plugin>
        </plugins>
    </build>
    <dependencies>
        <dependency>
            <groupId>org.citadel.features.distributed</groupId>
            <artifactId>org.citadel.features.distributed.dao-api</artifactId>
            <version>${project.version}</version>
        </dependency>
        <dependency>
            <groupId>org.osgi</groupId>
            <artifactId>osgi.core</artifactId>
            <version>${osgiVersion}</version>
        </dependency>
    </dependencies>
</project>
