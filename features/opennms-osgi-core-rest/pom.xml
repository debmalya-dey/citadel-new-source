<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://maven.apache.org/POM/4.0.0"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
    <parent>
        <relativePath>../topology-map/poms/compiled/</relativePath>
        <groupId>org.citadel.features.topology.build</groupId>
        <artifactId>compiled-bundle-settings</artifactId>
        <version>35.0.0-SNAPSHOT</version>
    </parent>
    <modelVersion>4.0.0</modelVersion>
    <groupId>org.citadel.osgi</groupId>
    <artifactId>citadel-osgi-core-rest</artifactId>
    <name>OpenNMS :: Features :: OSGI :: Core :: Rest</name>
    <description>
        Core module which provides features to install to listen for rest related services,
        such as @Path annotated interfaces.
    </description>
    <packaging>bundle</packaging>
    <properties>
        <bundle.namespace>org.citadel.osgi.core.rest</bundle.namespace>
        <bundle.symbolicName>${bundle.namespace}</bundle.symbolicName>
    </properties>
    <dependencies>
        <dependency>
            <groupId>org.slf4j</groupId>
            <artifactId>slf4j-api</artifactId>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>org.osgi</groupId>
            <artifactId>osgi.core</artifactId>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>org.osgi</groupId>
            <artifactId>osgi.cmpn</artifactId>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>com.eclipsesource.jaxrs</groupId>
            <artifactId>features</artifactId>
            <classifier>features</classifier>
            <type>xml</type>
            <version>${osgiJaxRsVersion}</version>
        </dependency>
        <dependency>
            <groupId>org.citadel.features</groupId>
            <artifactId>org.citadel.features.rest-provider</artifactId>
            <version>${project.version}</version>
        </dependency>
        <dependency>
            <groupId>org.citadel.container.bridge</groupId>
            <artifactId>org.citadel.container.bridge.rest</artifactId>
            <version>${project.version}</version>
        </dependency>
    </dependencies>
</project>
