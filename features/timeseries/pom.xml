    <dependency>
      <groupId>org.citadel.integration.api</groupId>
      <artifactId>config</artifactId>
      <version>${citadelApiVersion}</version>
      <scope>provided</scope>
    </dependency>
    <dependency>
      <groupId>org.citadel.core.ipc.rpc</groupId>
      <artifactId>org.citadel.core.ipc.rpc.utils</artifactId>
